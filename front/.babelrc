{
	"presets": ["next/babel"],
	"plugins": [
		[
			"styled-components",
			{
				"ssr": true,
				"preprocess": false,
				"fileName": true,
				"displayName": false,
				"pure": true
			}
		],
		[
			"prismjs",
			{
				"languages": ["javascript", "css", "html"],
				"plugins": ["line-numbers", "show-language"],
				"theme": "okaidia",
				"css": true
			}
		]
	]
}
